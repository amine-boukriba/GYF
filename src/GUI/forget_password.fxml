<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffc13b;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Forget_passwordController">
   <children>
      <Label layoutX="213.0" layoutY="40.0" text="Please enter your Email" />
      <VBox layoutX="184.0" layoutY="110.0" prefHeight="180.0" prefWidth="181.0" spacing="30.0" style="-fx-background-color: #F9E4B7;">
         <children>
            <TextField fx:id="email_text" promptText="email">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="send" mnemonicParsing="false" onAction="#receive_email" styleClass="search-button" stylesheets="@../stylesheet/style.css" text="send">
               <VBox.margin>
                  <Insets left="60.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
